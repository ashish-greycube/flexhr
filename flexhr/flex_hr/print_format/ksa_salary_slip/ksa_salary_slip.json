 "html": "{%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead) -%}\n\n    {% if letter_head and not no_letterhead %}\n\n    <div class=\"letter-head\">{{ letter_head }}</div>\n\n    {% endif %}\n\n    {%- if doc.meta.is_submittable and doc.docstatus==0-%}\n\n    <div class=\"alert alert-info text-center\">\n\n        <h4 style=\"margin: 0px;\">{{ _(\"DRAFT\") }}</h4></div>\n\n    {%- endif -%}\n\n    {%- if doc.meta.is_submittable and doc.docstatus==2-%}\n\n    <div class=\"alert alert-danger text-center\">\n\n        <h4 style=\"margin: 0px;\">{{ _(\"CANCELLED\") }}</h4></div>\n\n    {%- endif -%}\n\n    {% if max_pages > 1 %}\n\n    <p class=\"text-right\">{{ _(\"Page #{0} of {1}\").format(page_num, max_pages) }}</p>\n\n    {% endif %}\n\n{%- endmacro -%}\n\n \n\n{{ add_header(0,1,doc,letter_head, no_letterhead) }}\n\n<label>\n\n<p class=\"text-center\"><b>{{ _(\"Salary Slip\") }}</b></p><br>\n\n</label>\n\n \n\n<div class=\"row\">       \n\n            <div class=\"col-xs-3 text-right\"><label>Employee</label></div>\n\n            <div class=\"col-xs-3 \">{{ doc.employee }} </div>\n\n            <div class=\"col-xs-3 text-right\"><label>Start Date</label></div>\n\n            <div class=\"col-xs-3 \">{{ doc.start_date}}</div>      \n\n    </div>\n\n \n\n<div class=\"row\">       \n\n            <div class=\"col-xs-3 text-right\"><label>Employee Name</label></div>\n\n            <div class=\"col-xs-3 \">{{ doc.employee_name }} </div>\n\n            <div class=\"col-xs-3 text-right\"><label>End Date</label></div>\n\n            <div class=\"col-xs-3 \">{{ doc.end_date}}</div>      \n\n    </div>\n\n \n\n                <div class=\"row\">       \n\n            <div class=\"col-xs-3 text-right\"><label>Company</label></div>\n\n            <div class=\"col-xs-3 \">{{ doc.company}}</div>      \n\n                    <div class=\"col-xs-3 text-right\"><label>Working Days</label></div>\n\n            <div class=\"col-xs-3 \">{{ doc.total_working_days}} </div>\n\n          \n\n    </div>\n\n \n\n<div class=\"row\">       \n\n            <div class=\"col-xs-3 text-right\"><label>Department</label></div>\n\n            <div class=\"col-xs-3 \">{{ doc.department}}</div>      \n\n                    <div class=\"col-xs-3 text-right\"><label>Leave Without Pay</label></div>\n\n            <div class=\"col-xs-3 \">{{ doc.leave_without_pay}} </div>\n\n          \n\n    </div>\n\n \n\n<div class=\"row\">       \n\n            <div class=\"col-xs-3 text-right\"><label>Designation</label></div>\n\n            <div class=\"col-xs-3 \">{{ doc.designation}}</div>      \n\n                    <div class=\"col-xs-3 text-right\"><label>Payment Days</label></div>\n\n            <div class=\"col-xs-3 \">{{ doc.payment_days}} </div>\n\n          \n\n    </div>\n\n \n\n<div class=\"row\">       \n\n            <div class=\"col-xs-3 text-right\"><label>Branch</label></div>\n\n            <div class=\"col-xs-3 \">{{ doc.branch}}</div>      \n\n                    <div class=\"col-xs-3 text-right\"></div>\n\n            <div class=\"col-xs-3 \"></div>          \n\n    </div>\n\n \n\n \n\n<div class=\"row\">       \n\n    <div class=\"col-xs-6\">     \n\n \n\n<table class=\"table table-condensed table-hover table-bordered\">\n\n        <tr>\n\n            <th>Sr</th>\n\n            <th>Component</th>\n\n            <th class=\"text-right\">Amount</th>\n\n        </tr>\n\n        {%- for row in doc.earnings -%}\n\n        <tr>\n\n            <td style=\"width: 3%;\">{{ row.idx }}</td>\n\n            <td style=\"width: 67%; text-align: left;\">{{ row.salary_component }}</td>\n\n            <td style=\"width: 30%; text-align: right;\">               \n\n                {{ row.get_formatted(\"amount\", doc) or ''}} </td>\n\n        </tr>\n\n        {%- endfor -%}\n\n    </tbody>\n\n</table>\n\n</div>\n\n    <div class=\"col-xs-6\">\n\n<table class=\"table table-condensed table-hover table-bordered\">\n\n        <tr>\n\n            <th>Sr</th>\n\n            <th>Component</th>\n\n            <th class=\"text-right\">Amount</th>\n\n        </tr>\n\n       {%- for row in doc.deductions-%}\n\n        <tr>\n\n            <td style=\"width: 3%;\">{{ row.idx }}</td>\n\n            <td style=\"width: 67%; text-align: left;\">{{ row.salary_component }}</td>\n\n            <td style=\"width: 30%; text-align: right;\">                \n\n                {{ row.get_formatted(\"amount\", doc) or ''}} </td>\n\n        </tr>\n\n        {%- endfor -%}\n\n \n\n                <tr>\n\n                <td style=\"width: 3%;\">2</td>\n\n                <td style=\"width: 67%; text-align: left;\">{{ _(\"Loan\") }}</td>\n\n                <td style=\"width: 30%; text-align: right;\">{{ doc.get_formatted(\"total_loan_repayment\") or 0 }}</td>\n\n                </tr>\n\n \n\n    </tbody>\n\n</table>\n\n</div>\n\n<br>\n\n \n\n<div class=\"row\">       \n\n            <div class=\"col-xs-3 text-left\"><label>Gross Pay</label></div>\n\n            <div class=\"col-xs-3 \">{{ doc.gross_pay}}</div>      \n\n                    <div class=\"col-xs-3 text-right\"><label>Total Deduction</label></div>\n\n            <div class=\"col-xs-3 \">  {{ doc.total_deduction + doc.total_loan_repayment }}  </div>\n\n          \n\n    </div>\n\n<br>\n\n<div class=\"row\">       \n\n            <div class=\"col-xs-3 text-left\"><label>{{ _(\"Net Pay\") }}</label></div>\n\n            <div class=\"col-xs-3 \">{{ doc.net_pay }}</div>      \n\n                    <div class=\"col-xs-3 text-right\"><label>{{ _(\"Rounded Total\") }}</label></div>\n\n            <div class=\"col-xs-3 \">{{ doc.rounded_total}}  </div>\n\n          \n\n    </div>\n\n<br>\n\n  \n\n<div class=\"row\">\n\n<div class=\"col-xs-6 text-left\"><label>{{ _(\"Total in words\") }}</label></div>\n\n<div class=\"col-xs-6 text-left\">{{ doc.total_in_words }}</div>\n\n</div>", 
